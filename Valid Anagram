https://leetcode.com/problems/valid-anagram/

class Solution:
    def isAnagram(self, s, t):
        if len(s) != len(t):
            return False
        return sorted(s) == sorted(t)
solution = Solution()
s1 = "anagram"
t1 = "nagaram"
print(solution.isAnagram(s1, t1))
s2 = "rat"
t2 = "car"
print(solution.isAnagram(s2, t2)) 
